{
    "name": "hygen_scaffold_generator",
    "version": "1.0.0",
    "description": "Generate code scaffolding from your boilerplate code",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "repository": "",
    "author": "Andres Pajo",
    "license": "BSD-3-Clause",
    "type": "commonjs",
    "scripts": {
        "setup": "ln -s .hygen/dist/index.js ../.hygen.js",
        "start": "tsc -w",
        "build": "tsc",
        "serve": "nodemon --inspect --watch dist/ -d 1 ./dist/index.js",
        "postinstall": "yarn build",
        "test": "mocha -r ts-node/register src/test/*.ts"
    },
    "engines": {
        "node": "16.x"
    },
    "dependencies": {
        "@types/glob": "^8.0.0",
        "fs-extra": "^10.1.0",
        "glob": "^8.0.3",
        "hygen": "^6.2.11",
        "typescript": "^4.9.3"
    },
    "devDependencies": {
        "@types/chai": "^4.3.4",
        "@types/mocha": "^2.2.40",
        "chai": "^4.3.7",
        "child_process": "^1.0.2",
        "hygen-add": "^1.0.5",
        "mocha": "^3.3.0",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "tslint": "^5.2.0"
    }
}
